{
  "name": "atlc2-qualifier",
  "version": "0.0.0",
  "description": "\"The on-stream graphics used during the Amaz Team League Championship qualifier.\"",
  "homepage": "http://atlc2.org",
  "author": {
    "name": "Andrew Farrar",
    "email": "kegwen@gmail.com",
    "url": "http://kegwen.com"
  },
  "files": [
    "dashboard",
    "graphics",
    "extension.js",
    "extension"
  ],
  "keywords": [
    "nodecg",
    "hearthstone",
    "react",
    "nodecg-bundle"
  ],
  "nodecg": {
    "compatibleRange": "~0.8.0",
    "dashboardPanels": [
      {
        "name": "players",
        "title": "Players Editor",
        "width": 4,
        "file": "players.html",
        "headerColor": "#9f9bbd"
      },
      {
        "name": "series",
        "title": "Series Editor",
        "width": 4,
        "file": "series.html",
        "headerColor": "#9f9bbd"
      },
      {
        "name": "broadcast",
        "title": "Broadcast Editor",
        "width": 4,
        "file": "broadcast.html",
        "headerColor": "#9f9bbd"
      },
      {
        "name": "bracket",
        "title": "Bracket Editor",
        "width": 4,
        "file": "bracket.html",
        "headerColor": "#9f9bbd"
      }
    ],
    "graphics": [
      {
        "file": "up-next.html",
        "width": 1920,
        "height": 1080
      },
      {
        "file": "up-next-nostandings.html",
        "width": 1920,
        "height": 1080
      },
      {
        "file": "caster-cams-overview.html",
        "width": 1920,
        "height": 1080
      },
      {
        "file": "caster-cams-midseries.html",
        "width": 1920,
        "height": 1080
      },
      {
        "file": "in-game.html",
        "width": 1920,
        "height": 1080
      },
      {
        "file": "flyout.html",
        "width": 1920,
        "height": 100
      }
    ],
    "assetCategories": [
      {
        "name": "player-pictures",
        "title": "Player Pictures",
        "allowTypes": [
          "jpg",
          "jpeg",
          "gif",
          "png"
        ]
      },
      {
        "name": "caster-pictures",
        "title": "Caster Pictures",
        "allowTypes": [
          "jpg",
          "jpeg",
          "gif",
          "png"
        ]
      }
    ]
  },
  "repository": "https://github.com/nodecg/nodecg.git",
  "license": "MIT",
  "dependencies": {
  }
}
